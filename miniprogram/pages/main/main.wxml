 
  <!-- 数据 -->
  <view class="count">
    <view class="cell">{{datanum1}}°C<text>当前箱内温度</text></view>
    <view  class="cell">{{datanum2}}%<text>当前箱内湿度</text></view>
  </view>

  <view style="background-color: #ffffff; border-radius: 82rpx 82rpx 0 0;">

  <!-- 记录 -->
<view style="margin-top: 9%;">
