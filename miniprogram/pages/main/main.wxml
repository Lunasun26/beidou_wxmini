      <view class="weui-cell_bd">温度记录</view>
  </view>
  <view class="charts-box">
    <qiun-wx-ucharts 
    type="area"
      opts="{{opts1}}"
      chartData="{{chartData1}}"
      type="column"
      opts="{{opts}}"
      chartData="{{chartData}}"
    />
  </view>
  <view  class="weui">
      <view class="weui-cell_bd">湿度记录</view>
  </view>
  <view class="charts-box">
    <qiun-wx-ucharts 
      type="area"
      opts="{{opts1}}"
      chartData="{{chartData1}}"
    />
  </view>
</view>
</view>